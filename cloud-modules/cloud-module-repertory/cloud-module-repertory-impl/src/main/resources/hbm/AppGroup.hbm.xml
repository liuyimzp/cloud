<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yinhai.cloud.module.repertory.impl.po.AppGroupPo" table="CLOUD_APP_Group" dynamic-insert="true"
           dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="ID" length="10"/>
            <generator class="native"/>
        </id>
        <property name="businessArea" type="java.lang.String">
            <column name="BUSINESSAREA" length="6"/>
        </property>
        <property name="groupIdentify" type="java.lang.String">
            <column name="GROUPIDENTIFY" length="30"/>
        </property>
        <property name="groupName" type="java.lang.String">
            <column name="GROUPNAME" length="50"/>
        </property>
    </class>
</hibernate-mapping>