<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yinhai.cloud.module.application.impl.po.ConfigTemplatePo" table="cloud_config_template" dynamic-insert="true"
           dynamic-update="true">
        <id name="configTemplateId" type="java.lang.Long">
            <column name="cloud_config_template_id" length="20"/>
            <generator class="native"/>
        </id>
        <property name="middlewareType" type="java.lang.String">
            <column name="cloud_config_template_type"/>
        </property>
        <property name="configTemplateContent" type="java.lang.String">
            <column name="cloud_config_template_content"/>
        </property>
    </class>
</hibernate-mapping>
