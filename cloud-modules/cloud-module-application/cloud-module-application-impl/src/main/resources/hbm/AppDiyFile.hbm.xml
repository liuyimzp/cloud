<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yinhai.cloud.module.application.impl.po.AppDiyFileVo" table="CLOUD_APP" dynamic-insert="true"
           dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="ID" length="10"/>
            <generator class="native"/>
        </id>
        <property name="appDiyFile" type="java.lang.String">
            <column name="APPDIYFILE" />
        </property>
        <property name="appStatus" type="java.lang.String">
            <column name="APPSTATUS" />
        </property>
    </class>
</hibernate-mapping>
