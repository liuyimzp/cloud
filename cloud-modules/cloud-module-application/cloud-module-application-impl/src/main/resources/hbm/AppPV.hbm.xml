<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yinhai.cloud.module.application.impl.po.AppPVPo" table="CLOUD_APP_PV" dynamic-insert="true"
           dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="ID" length="10"/>
            <generator class="native"/>
        </id>
        <property name="appId" type="java.lang.Long">
            <column name="APPID" length="10"/>
        </property>
        <property name="volumeId" type="java.lang.Long">
            <column name="VOLUME_ID" length="10"/>
        </property>
        <property name="path" type="java.lang.String">
            <column name="PATH" length="50"/>
        </property>
        <property name="space" type="java.lang.Integer">
            <column name="SPACE"/>
        </property>
    </class>
</hibernate-mapping>
